    "using DifferentialEquations\n",
    "\n",
    "A = Matrix{Float64}([\n",
    "    1  0  0 -5;\n",
    "    4 -2  4 -3;\n",
    "   -4  0  0  1;\n",
    "    5 -2  2  3\n",
    "])\n",
    "\n",
    "u0 = rand(4,2)\n",
    "t_span = (0.0, 1.0)\n",
    "f(u, p, t) = A*u\n",
    "prob = ODEProblem(f, u0, t_span)\n",
    "sol = solve(prob)"
   ]
